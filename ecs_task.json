{
  "family": "{{ECSTaskDefinitionName}}",
  "networkMode": "bridge",
  "containerDefinitions": [
    {   
      "memory": 512,
      "portMappings": [
        {   
          "hostPort": 8080,
          "containerPort": 8080,
          "protocol": "tcp"
        }   
      ],  
      "essential": true,
      "name": "maraudersmap",
      "image": "{{DISTELLI_DOCKER_REPO}}:{{DOCKERTAG}}",
      "cpu": 512 
    },  
    {   
      "portMappings": [
        {   
          "hostPort": 6379,
          "containerPort": 6379,
          "protocol": "tcp"
        }   
      ],  
      "essential": true,
      "name": "redis",
      "image": "redis:3.2",
      "cpu": 0,
      "memoryReservation": 256 
    }   
  ]
}
